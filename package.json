{
  "name": "artistic-components",
  "version": "0.7.0",
  "type": "module",
  "author": "snorlax.xu",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "patch": "npm version patch && git tag v$(node -p -e \"require('./package.json').version\") && git push origin main --tags",
    "minor": "npm version minor && git tag v$(node -p -e \"require('./package.json').version\") && git push origin main --tags",
    "dev": "npm run storybook",
    "build": "tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 'src/**/*.{css,png,d.ts}' 'dist/'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/addon-onboarding": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/react-vite": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "storybook": "^8.1.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
